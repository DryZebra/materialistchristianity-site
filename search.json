---
layout: none
---
[
  {% assign first = true %}
  {% for page in site.pages %}
    {% if page.url contains '/maintenance-log/' and page.url != '/maintenance-log/' and page.url != '/maintenance-log/index.html' %}
      {% unless first %},{% endunless %}
      {
        "title": "{{ page.title | escape }}",
        "url": "{{ page.url | relative_url }}"
      }
      {% assign first = false %}
    {% endif %}
  {% endfor %}
]
