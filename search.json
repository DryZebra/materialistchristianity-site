---
layout: none
---
{% assign log_pages = site.pages | where_exp: "page", "page.url contains '/maintenance-log/' and page.url != '/maintenance-log/' and page.url != '/maintenance-log/index.html'" %}
[
  {% for page in log_pages %}
  {
    "title": "{{ page.title | escape }}",
    "url": "{{ page.url | relative_url }}",
    "content": {{ page.content | strip_html | normalize_whitespace | jsonify }}
  }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
